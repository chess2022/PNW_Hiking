<!DOCTYPE html>
<html>
<%- include("partials/head.ejs") %>
<body>
    <%- include("partials/user_header.ejs") %>
    <h2><%= currentUser %>'s Saved Hikes </h2>
    <div class="results">
            <div class="list" id="trails">
                
                <% allSaved.forEach(save => { %>
                <a id="trailBoxes" href="/user/saved/<%=save._id; %>"> 
                    <div id="trailImg"><img src="<%=save.image; %>"></div>
                    <div id="trailName"><h4><%= save.name %></h4></div>
                    <div id="trailNotes"><p><%= save.note %></p></div>
                    <button id="addNote">Add note</button>
                </a>
                 <% }) %>
            </div>  
    </div>
</body>
</html>